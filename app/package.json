    "@types/color": "^3.0.0",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.15",
    "@types/react-native-fbsdk": "^1.0.0",
    "@types/react-native-vector-icons": "^6.4.2",
    "@types/react-navigation": "^3.0.8",
    "@types/styled-components": "^4.1.19",
    "color": "^3.1.2",
    "date-fns": "^2.2.1",
    "fuse.js": "^3.4.5",
    "react": "16.8.6",
    "react-native": "^0.60.5",
    "react-native-code-push": "^5.7.0",
