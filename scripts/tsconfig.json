{
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2015",
    "noEmit": true,
    "typeRoots": ["typings"]
  }
}
